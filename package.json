{
  "name": "gflow",
  "version": "1.0.0",
  "description": "The aim of this project is to provide a complete process chain to organize the releases of your projects. It builds on the old and proven principles of git flow, semantic versioning (SemVer) and conventional commits. But offers a well-coordinated overall concept that integrates all of these principles.",
  "main": "build/main.js",
  "scripts": {
    "start": "node build/src/Main.js",
    "compile": "tsc",
    "clean": "git clean -fXd -e !node_modules -e !node_modules/**/*",
    "build": "npm run clean && npm run compile",
    "lint": "eslint . --ext .ts",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "docs": "typedoc --options typedoc.json",
    "make": "npm run build && npm run lint && npm run test && npm run docs",
    "feature:start": "node build/src/Main.js feature start",
    "feature:finish": "node build/src/Main.js feature finish",
    "release:start": "node build/src/Main.js release start",
    "release:finish": "node build/src/Main.js release finish",
    "hotfix:start": "node build/src/Main.js hotfix start",
    "hotfix:finish": "node build/src/Main.js hotfix finish",
    "bugfix:start": "node build/src/Main.js bugfix start",
    "bugfix:finish": "node build/src/Main.js bugfix finish",
    "support:start": "node build/src/Main.js support start",
    "support:finish": "node build/src/Main.js support finish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CuddlySheep/node-gflow.git"
  },
  "keywords": [
    "git",
    "git-flow",
    "standard-version",
    "semantic versioning",
    "SemVer",
    "conventional commits"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CuddlySheep/node-gflow/issues"
  },
  "homepage": "https://github.com/CuddlySheep/node-gflow#readme",
  "dependencies": {
    "@types/concat-stream": "^1.6.0",
    "@types/conventional-commits-parser": "^3.0.0",
    "@types/semver": "^7.2.0",
    "@types/standard-version": "^7.0.0",
    "commander": "^5.1.0",
    "concat-stream": "^2.0.0",
    "conventional-commits-parser": "^3.1.0",
    "semver": "^7.3.2",
    "simple-git": "^2.5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/fs-extra": "^8.1.1",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-tsdoc": "^0.2.4",
    "fs-extra": "^9.0.0",
    "mocha": "^7.1.2",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.7",
    "typedoc-plugin-external-module-map": "^1.2.1",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.9.2"
  }
}
